{
  "name": "lichi/deploy_daemon_bot",
  "type": "library",
  "description": "Deploy daemon bot",
  "keywords": ["Deploy","deploy bot", "deploy daemon"],
  "homepage": "https://github.com/IvanHunters/deploy_daemon_bot",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikovivan@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "php": ">=7.3"
  },
  "autoload": {
    "psr-4": {
      "Daemon\\": "src/",
      "Daemon\\Sdk\\": "src/sdk/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^4.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}